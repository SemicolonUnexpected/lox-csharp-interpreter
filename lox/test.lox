print "";

print "Approximating pi...";

var pi = 0;
var maximum = 10000;
var counter = 0;
while (counter < maximum) {
    pi = pi + 1 / (counter * 4 + 1) - 1 / ((counter + 1) * 4 - 1);
    counter = counter + 1;
}

print pi * 4;
